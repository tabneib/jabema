import serversidechannel.Hash;

		
	pointcut pcQUIT(ftpdProtocol protocol, String whatever > String):
		execution(String QUIT(String))
		&& target(protocol)
		&& args(whatever);

	pointcut pcSTOR(ftpdProtocol protocol, String fileName > String):
		execution(String STOR(String))
		&& target(protocol)
		&& args(fileName);

	pointcut pcDELE(ftpdProtocol protocol, String fileName > String):
		execution(String DELE(String))
		&& target(protocol)
		&& args(fileName);
	
	pointcut pcRNTO(ftpdProtocol protocol, String fileName > String):
		execution(String RNTO(String))
		&& target(protocol)
		&& args(fileName);
	
	pointcut pcRNFR(ftpdProtocol protocol, String fileName > String):
		execution(String RNFR(String))
		&& target(protocol)
